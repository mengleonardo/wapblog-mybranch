    <servlet-name>EditBlogSuccess</servlet-name>
    <servlet-class>servlet.EditBlogSuccessServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditBlogSuccess</servlet-name>
    <url-pattern>/edit-blog-success</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BlogList</servlet-name>
    <servlet-class>servlet.BlogListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BlogList</servlet-name>
    <url-pattern>/list</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ViewBlogServlet</servlet-name>
    <servlet-class>servlet.ViewBlogServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewBlogServlet</servlet-name>
    <url-pattern>/view</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DeleteBlogServlet</servlet-name>
    <servlet-class>servlet.DeleteBlogServlet</servlet-class>
