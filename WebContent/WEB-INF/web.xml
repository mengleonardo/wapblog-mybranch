    <servlet-class>servlet.LoginVerifiedServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginVerifiedServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RegisterVerifiedServlet</servlet-name>
    <servlet-class>servlet.RegisterVerifiedServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterVerifiedServlet</servlet-name>
    <url-pattern>/registerVerified</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>NewBlogServlet</servlet-name>
    <servlet-class>servlet.NewServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NewBlogServlet</servlet-name>
    <url-pattern>/new</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EditBlog</servlet-name>
    <servlet-class>servlet.EditBlogServlet</servlet-class>
  </servlet>
